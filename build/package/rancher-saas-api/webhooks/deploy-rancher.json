[
  {
    "id": "deploy",
    "execute-command": "/opt/webhook-scripts/deploy-rancher.bash",
    "include-command-output-in-response": true,
    "pass-arguments-to-command":
    [
      {
        "source": "payload",
        "name": "instance_name"
      },
      {
        "source": "payload",
        "name": "instance_size"
      },
      {
        "source": "payload",
        "name": "admin_password"
      },
      {
        "source": "payload",
        "name": "job_id"
      }
    ],
    "trigger-rule":
    {
      "match":
      {
        "type": "value",
        "value": "<YOUR-GENERATED-TOKEN>",
        "parameter":
        {
          "source": "payload",
          "name": "token"
        }
      }
    }
  }
]
